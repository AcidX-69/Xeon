-- Scripts:

game.StarterGui:SetCore("SendNotification",{Title = "Credit",Text = "Made by Acid"})

-- Open / Close -- 

Close.MouseButton1Click:connect(function()
	Main.Visible = false
	Opengui.Visible = true
end)

open.MouseButton1Click:connect(function()
	Main.Visible = true
	Opengui.Visible = false
end)

-- Teleports --
insidePrison.MouseButton1Click:connect(function()
	game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(924.4, 97.1383, 2388.5)
end)

outsidePrison.MouseButton1Click:connect(function()
	game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(452, 96, 2290)
end)

